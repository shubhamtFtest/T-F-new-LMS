                    <aura:if isTrue="{!!v.showproductSearch}">
                        <div class="c-container">
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <lightning:input name="input8" value="{!v.ISBNValue}" placeholder="{!v.IsbnLabel}" label="{!v.IsbnLabel}" class="sfdc_usernameinput"/>
                                    </div>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small" >
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <lightning:input name="input8" value="{!v.productTitle}" placeholder="{!v.TitleLabel}" label="{!v.TitleLabel}" class="sfdc_usernameinput"/>
                                    </div>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <lightning:input name="input8" value="{!v.productAuthor}" placeholder="{!v.AuthorLabel}" label="{!v.AuthorLabel}" class="sfdc_usernameinput"/>
                                    </div>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <!--<div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <lightning:input name="input8" value="" placeholder="{!v.CatalogueNumberLabel}" label="{!v.CatalogueNumberLabel}" class="sfdc_usernameinput"/>
                                    </div>-->
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="6" padding="around-small">
                                    <c:TF_AG_NetbaseDualBoxComp values="{!v.selectedNetbaseValues}" backToProductDetail="{!v.eventFromProdDetail}" checkedValuesListBox="{!v.multiResultCodes}"/>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="6" padding="around-small">
                                    <c:SubjectGrid checkedValuesListBox="{!v.multiResultList}"/>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Not Yet Published</p>
                                        <aura:iteration items="{!v.lstPublished}" var="published">
                                            <lightning:input type="radio" label="{!published.name}" name="radio1" value="{!published.value}" checked="{!published.isChecked}" onchange="{!c.selectpublish}"/>
                                        </aura:iteration>
                                    </div>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Medium</p>
                                        <aura:iteration items="{!v.lstMedium}" var="medium">
                                            <lightning:input disabled = "true" type="radio" label="{!medium.name}" name="radio" value="{!medium.value}" onchange="{!c.selectMedium}" checked="{!medium.isChecked}"/>
                                        </aura:iteration>
                                    </div>
                                </lightning:layoutItem>
                                
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Publication Price</p>
                                        <lightning:input type="number" label="" variant = "label-hidden" name="input1" placeholder="Minimum" value="{!v.minPrice}"/><br/>
                                        <lightning:input type="number" label="" variant = "label-hidden" name="input1" placeholder="Maximum" value="{!v.maxPrice}"/>
                                    </div> 
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Publication Date</p>
                                        <lightning:input type="date" variant = "label-hidden" placeholder="From" value="{!v.pubFromDate}" max="{!v.pubToDate}" /><br/>
                                        <lightning:input type="date" variant = "label-hidden" placeholder="To" value="{!v.pubToDate}" min="{!v.pubFromDate}"/>
                                    </div>
                                </lightning:layoutItem>
                                
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Has DRM</p>
                                        <aura:iteration items="{!v.lstHasDRM}" var="DRM">
                                            <lightning:input disabled="true" type="radio" label="{!DRM.name}" name="radio3" value="{!DRM.value}" checked="{!DRM.isChecked}" onchange="{!c.selectDRM}"/>
                                        </aura:iteration>
                                    </div>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <!--<label class="slds-form-element__label" for="select-01"><h1 style="font-size:16px"><b>Excluded Country</b></h1></label>
                                    <ui:inputSelect aura:id="SeriesValue" class="slds-select" label="" change="{!c.onSeriesCountry}"/>
                                    <lightning:combobox class="" name="Excluded Country" label="Excluded Country" value="{!v.defaultCountryCode}" placeholder="Select Country" options="{! v.lstCountryCode}" onchange="{! c.selectCountry }"/>-->
                                    <lightning:select disabled="true" class="" name="" label="Excluded Country" value="" onchange="{!c.onSeriesCountry}" >
                                        <aura:iteration items="{!v.countryList}" var="color">
                                            <option value="{!color.value}" text="{!color.label}" class="" />
                                        </aura:iteration>
                                    </lightning:select>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <!--<div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <p class="title">Publication Price</p>
                                        <aura:iteration items="{!v.lstPublicationPrice}" var="pubprice">
                                            <lightning:input type="radio" label="{!pubprice.name}" name="radio2" value="{!pubprice.value}" checked="{!pubprice.isChecked}" onchange="{!c.selectCurrency}"/>
                                        </aura:iteration>
                                    </div>-->
                                    <!--<lightning:combobox class="" name="Publication Price" label="Publication Price" value="{!v.defaultPubPriceValue}" placeholder="Select Publication Price" options="{! v.lstPublicationPrice}" onchange="{! c.selectCurrency }"/>-->
                                </lightning:layoutItem>
                                <!-- <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="4" largeDeviceSize="3" padding="around-small">
                                    <div class="slds-form-element" onkeypress="{!c.searchProductEnter}">
                                        <lightning:input name="input50" value="{!v.FirstPublishedYearValue}" placeholder="{!v.FirstPublishedYearLabel}" label="{!v.FirstPublishedYearLabel}" class="sfdc_usernameinput"/>
                                    </div>
                                </lightning:layoutItem> -->
                            </lightning:layout>
                            <br/>
                            <div class="srchBtn">
                                <lightning:button variant="success" disabled="{!v.syncProductDataWrapperCheckboxAdd.length == 0}" label="Add Selected" title="Add Selected" onclick="{! c.finalSyncSelectedProds }" />
                                <lightning:button variant="brand" disabled="{!v.syncProductDataWrapper.length == 0}" label="Add/Remove Products" title="Add/Remove Products" onclick="{! c.openModel }" />
                                <lightning:button variant="brand" label="Reset All" title="Reset All" onclick="{!c.ResetAllData}"/>
                                <lightning:button variant="brand" label="{!v.searchProductTitle}" title="{!v.searchProductTitle}" onclick="{! c.searchProduct}" disabled="{!v.disableSearch}"/>
                                <lightning:button variant="brand" label="Product Data Report" title="Product Data Report" onclick="{!c.openProductDataReport}"/>
                            </div>
                            
                        </div>
                        
                    </aura:if>
                    
                </lightning:tab>
                <lightning:tab label="Multi ISBN Search" id="multiIsbnTab" onactive="{! c.handleActive }"> 
                    <lightning:tabset variant="vertical" selectedTabId="{!v.setSelectedChildTab}">
                        <lightning:tab label="ISBN List" id="isbnListTab" onactive="{! c.handleChildTabActive }">
                            <div class="c-container">
                                <lightning:layout multipleRows="true">
                                    <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="6" largeDeviceSize="6" padding="around-small">
                                        <div id="container">
                                            <div class="left">
                                                <ui:inputTextArea aura:id="multiIsbn" placeholder="ISBN List" value="{!v.multiISBNValues}" rows="5" maxlength="1900"/>
                                            </div>
                                            <div class="right">
                                                <lightning:helptext content="ISBNs must be on separate lines."/>
                                            </div>
                                        </div>
                                    </lightning:layoutItem>
                                    <lightning:layoutItem size="12" smallDeviceSize="6" mediumDeviceSize="6" largeDeviceSize="6" padding="around-small">
                                        <lightning:button variant="brand" label="{!v.searchProductMultiISBN}" title="{!v.searchProductMultiISBN}" onclick="{!c.searchMultiIsbn}" disabled="{!v.disableMultiIsbnSearchBtn}"/>
